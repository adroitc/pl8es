<%= form_for @menu, validate: true do |f| %>
	<%= f.text_field :title %>
	<%= check_box_tag :default %>
	<%= f.text_field :from_time, :class => "timepicker", :data => { "default-time" => false, "show-meridian" => false} %>
	<%= f.text_field :to_time, :class => "timepicker", :data => { "default-time" => false, "show-meridian" => false} %>
	<%= f.submit %>
<% end %>